  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "src/extension.js",
  "devDependencies": {
    "@vscode/vsce": "3.2.1",
    "prettier": "3.4.2",
    "rimraf": "6.0.1"
  }
}
