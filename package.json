{
  "name": "macsploit-execute",
  "displayName": "MacSploit Execute",
  "author": "Snipcola",
  "publisher": "Snipcola",
  "version": "0.0.6",
  "description": "Execute scripts using MacSploit.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://git.snipcola.com/snipcola/MacSploit-Execute.git"
  },
  "scripts": {
    "build": "rimraf out && mkdir out && vsce package --out out/extension.vsix",
